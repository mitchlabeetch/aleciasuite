{
  "kind": "collectionType",
  "collectionName": "partner_logos",
  "info": {
    "singularName": "partner-logo",
    "pluralName": "partner-logos",
    "displayName": "Partner Logo",
    "description": "Logos of partners, banks, funds, and advisors"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "logo": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": ["images"]
    },
    "url": {
      "type": "string",
      "description": "Website URL of the partner"
    },
    "category": {
      "type": "enumeration",
      "enum": ["bank", "fund", "advisory", "partner", "client"],
      "required": true,
      "default": "partner"
    },
    "quality": {
      "type": "enumeration",
      "enum": ["high", "medium", "low"],
      "default": "medium",
      "description": "Logo image quality"
    },
    "format": {
      "type": "enumeration",
      "enum": ["svg", "png", "jpg", "webp"],
      "default": "png"
    },
    "display_order": {
      "type": "integer",
      "default": 0,
      "required": true
    },
    "is_visible": {
      "type": "boolean",
      "default": true,
      "required": true
    }
  }
}
