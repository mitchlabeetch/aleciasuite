{
    "name": "alepanel",
    "version": "2.0.0",
    "private": true,
    "packageManager": "pnpm@10.28.0",
    "scripts": {
        "dev": "turbo run dev",
        "build": "turbo run build",
        "lint": "turbo run lint",
        "dev:website": "npm run dev --workspace=apps/website",
        "build:website": "npm run build --workspace=packages/ui && npm run build --workspace=apps/website",
        "dev:colab": "npm run dev --workspace=apps/colab",
        "build:colab": "npm run build --workspace=apps/colab"
    },
    "dependencies": {
        "@dnd-kit/core": "^6.3.1",
        "@dnd-kit/sortable": "^10.0.0",
        "@dnd-kit/utilities": "^3.2.2",
        "@microsoft/microsoft-graph-client": "^3.0.7",
        "@types/html-to-text": "^9.0.4",
        "groq-sdk": "^0.11.0",
        "html-to-text": "^9.0.5",
        "isomorphic-fetch": "^3.0.0",
        "jsondiffpatch": "^0.7.3",
        "mathjs": "^12.4.1",
        "openai": "^4.28.4",
        "papaparse": "^5.4.1",
        "pdf-lib": "^1.17.1",
        "pg": "^8.18.0",
        "uuid": "^13.0.0"
    },
    "workspaces": [
        "apps/*",
        "packages/*",
        "services/*"
    ],
    "devDependencies": {
        "@types/node": "^20",
        "@types/papaparse": "^5.5.2",
        "@types/prosemirror-model": "^1.17.0",
        "@types/react": "^19",
        "@types/uuid": "^11.0.0",
        "next": "^15.3.6",
        "turbo": "2.8.9"
